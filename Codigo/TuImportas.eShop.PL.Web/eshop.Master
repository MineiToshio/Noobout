    <!--  = Login =  -->
    <asp:Panel runat="server" DefaultButton="btnLogin" id="loginModal" class="modal hide fade validationEngineContainer" tabindex="-1" role="dialog" aria-labelledby="loginModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="loginModalLabel"><span class="light">Ingresar</span> a Noobout</h3>
        </div>
        <div class="modal-body" style="overflow-y:visible;">
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="inputEmail">Usuario</label>
                <div class="controls">
                    <asp:TextBox runat="server" ID="txtLoginUsuario" CssClass="input-block-level validate[required]" placeholder="Usuario"></asp:TextBox>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="inputPassword">Contraseña</label>
                <div class="controls">
                    <asp:TextBox runat="server" TextMode="Password" ID="txtLoginContrasena" CssClass="input-block-level validate[required]" placeholder="Contraseña"></asp:TextBox>
                </div>
            </div>
            <%--<div class="control-group">
                <div class="controls">
                    <label class="checkbox">
                        <input type="checkbox">
                        Remember me
                    </label>
                </div>
            </div>--%>
            <%--<button type="submit" class="btn btn-primary input-block-level bold higher">
                INGRESAR
            </button>--%>
            <center>
                <asp:Button runat="server" ID="btnLogin" CssClass="btn btn-primary input-block-level bold higher" Text="INGRESAR" OnClick="btnLogin_Click"/><%-- style="display:none;"--%>
                <%--<input type="button" ID="btnLoginAux" class="btn btn-primary input-block-level bold higher" value="INGRESAR" onclick="Login()" />--%>
            </center>
            <br />
            <p class="center-align push-down-0">
                <a data-toggle="modal" role="button" href="#forgotPassModal" data-dismiss="modal">¿Olvdaste tu contraseña?</a>
            </p>
        </div>
    </asp:Panel>

    <!--  = Register =  -->
    <div id="registerModal" class="modal hide fade validationEngineContainer" tabindex="-1" role="dialog" aria-labelledby="registerModalLabel" aria-hidden="true" onkeypress="EnterRegistrar();">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="registerModalLabel"><span class="light">Registrarse</span> a Noobout</h3>
        </div>
        <div class="modal-body" style="overflow-y:visible;">
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroNombre">Nombre</label>
                <div class="controls">
                    <input type="text" class="input-block-level validate[required]" id="txtRegistroNombre" placeholder="Nombre" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroApellidoP">Apellido Paterno</label>
                <div class="controls">
                    <input type="text" class="input-block-level validate[required]" id="txtRegistroApellidoP" placeholder="Apellido Paterno" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroDNI">DNI</label>
                <div class="controls">
                    <input type="text" class="input-block-level positive-integer validate[required]" id="txtRegistroDNI" placeholder="DNI" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroUsuario">Usuario</label>
                <div class="controls">
                    <input type="text" class="input-block-level validate[required]" id="txtRegistroUsuario" placeholder="Usuario" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroEmail">Email</label>
                <div class="controls">
                    <input type="Email" class="input-block-level validate[required,custom[email]]" id="txtRegistroEmail" placeholder="Email" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroContrasena">Contraseña</label>
                <div class="controls">
                    <input type="Password" class="input-block-level validate[required]" id="txtRegistroContrasena" placeholder="Contraseña" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtRegistroContrasena2">Repetir Contraseña</label>
                <div class="controls">
                    <input type="password" class="input-block-level validate[required,equals[txtRegistroContrasena]]" id="txtRegistroContrasena2" placeholder="Repetir Contraseña" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label hidden shown-ie8" for="txtCaptcha">Captcha</label>
                <div class="controls">
                    <asp:Image ID="imgCaptcha" runat="server" ImageUrl="~/CImage.aspx" Style="width:60%;float: left;height: 37px;"/>
                    <input type="text" class="input-block-level validate[required]" id="txtCaptcha" placeholder="Captcha" style="width:40%"/>
                </div>
            </div>
            <%--<button type="submit" class="btn btn-danger input-block-level bold higher" onclick="RegistrarUsuario();">
                REGISTRARSE
            </button>--%>
            
            <p class="push-down-0" style="float:left;padding-top:9px;margin-right:1px;"><a data-toggle="modal" role="button" href="#loginModal" data-dismiss="modal">¿Ya eres miembro?</a></p>
            <input id="btnRegistrar" type="button" class="btn btn-danger input-block-level bold higher" onclick="RegistrarUsuario();" value="REGISTRATE" />
        </div>
    </div>

    <!--  = Forgot your password =  -->
    <div id="forgotPassModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="forgotPassModalLabel" aria-hidden="true">
