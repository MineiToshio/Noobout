        <script type="text/javascript">
            $(".positive-integer").numeric({ decimal: false, negative: false }, function () { alert("Positive integers only"); this.value = ""; this.focus(); });

            function SeleccionarMenu(menu)
            {
                //$("#liMenuInicio").removeClass("active");
                //$("#liMenuTienda").removeClass("active");
                //$("#liMenuNosotros").removeClass("active");
                //$("#liMenuContacto").removeClass("active");

                $("[id*=liMenu]").removeClass("active");

                $("#" + menu).addClass("active");
            }

            function Login()
            {
                if ($("#loginModal").validationEngine('validate')) {
                    <%= Page.ClientScript.GetPostBackEventReference(btnLogin, "") %>
                }
            }

            function RegistrarUsuario()
            {
                if ($("#registerModal").validationEngine('validate')) {
                    var objUsuarioBE = {};

                    objUsuarioBE.Nombre = $("#txtRegistroNombre").val(),
                    objUsuarioBE.Apellido_Paterno = $("#txtRegistroApellidoP").val(),
                    objUsuarioBE.Dni = $("#txtRegistroDNI").val(),
                    objUsuarioBE.Usuario = $("#txtRegistroUsuario").val(),
                    objUsuarioBE.Email = $("#txtRegistroEmail").val(),
                    objUsuarioBE.Password = $("#txtRegistroContrasena").val(),
                    objUsuarioBE.Captcha = $("#txtCaptcha").val()

                    var DTO = { 'objUsuarioBE': objUsuarioBE };

                    $.ajax
                    ({
                        type: "POST",
                        url: pageUrl + "/RegistrarUsuario",
                        data: JSON.stringify(DTO),
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (response) {
                            switch (response.d) {
                                case -1: //Usuario_Repetido
                                    jAlert("Lo sentimos, el usuario que ha escogido ya se encuentra registrado.", "Usuario Repetido");
                                    ReloadCaptcha();
                                    break;
                                case -2: //Email_Repetido
                                    jAlert("Lo sentimos, el email que ha escogido ya se encuentra registrado.", "Email Repetido");
                                    ReloadCaptcha();
                                    break;
                                case -3: //Dni_Repetido
                                    jAlert("Lo sentimos, el DNI que ha escogido ya se encuentra registrado.", "DNI Repetido");
                                    ReloadCaptcha();
                                    break;
                                case -4: //Dni_Repetido
                                    jAlert("El código captcha se encuentra errado.", "Captcha Errado");
                                    ReloadCaptcha();
                                    break;
                                default:
                                    jAlert("Su cuenta ha sido creada satisfactoriamente. Por favor revise su correo para activar su cuenta.", "Cuenta Creada");
                                    break;
                            }
                        },
                        error: function (response) {
                            MostrarError();
                        }
                    });
                }
            }

            $(document).ajaxStart($.blockUI).ajaxStop($.unblockUI);

            function ReloadCaptcha()
            {
                d = new Date();
                $("#imgCaptcha").attr("src", "CImage.aspx?" + d.getTime());
            }

            function Notificacion(texto)
            {
                var n = noty({
                    text: texto,
                    type: 'warning',
                    dismissQueue: true,
                    layout: 'topCenter',
                    theme: 'defaultTheme',
                    closeWith: ['button']
                });
            }

            $(document).ready(function () {
                if($('#<%=hdVerifico.ClientID%>').val() == 0)
                    Notificacion('Por favor, verifique su correo. Para volver a enviar el correo de verificación <a href=\'/Cuenta/1\'>click aquí</a>')

                $("#registerModal").validationEngine({ scroll: false, autoHidePrompt:true });
                $("#atributosProductoModal").validationEngine({ scroll: false, autoHidePrompt: true });
            });

            function EnterLogin()
            {
                if (event.keyCode == '13') {
                    $("#btnLoginAux").click();
                    event.preventDefault();
                }
            }

            function EnterRegistrar() {
                if (event.keyCode == '13') {
                    $("#btnRegistrar").click();
                    event.preventDefault();
                }
            }

            function QuitarCarrito(idProducto, idAtributos)
            {
                $.ajax
                ({
                    type: "POST",
                    url: pageUrl + "/QuitarCarrito",
                    data: "{'id_producto': '" + idProducto + "', 'idAtributos': '" + idAtributos + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        $("#pnlItemCart" + idProducto + "_" + idAtributos).remove();
                        $("#lblSubTotalCarrito").text(Redondear(parseFloat($("#lblSubTotalCarrito").text()) - parseFloat(response.d)));
                        //$("#lblCostoCarrito").text(parseFloat($("#lblSubTotalCarrito").text()) + parseFloat($("#lblPrecioEnvio").text()));
                        $("#lblCostoCarrito").text(parseFloat($("#lblSubTotalCarrito").text()));
                        $("#lblCantidadCarrito").text(parseInt($("#lblCantidadCarrito").text()) - 1);

                        //if(parseInt($("#lblCantidadCarrito").text()) == 0)
                        //    $("#pnlCarrito").css("display", "none");
                    },
                    error: function (response) {
                        MostrarError();
                    }
                });
            }

            function AgregarCarrito(idProducto, atributos) {
                $.ajax
                ({
                    type: "POST",
                    url: pageUrl + "/AgregarCarrito",
                    data: "{'id_producto': '" + idProducto + "', 'atributos': '" + atributos + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response.d.control != "") {
                            $("#pnlItemsCarrito").append(response.d.control);
                            $("#lblSubTotalCarrito").text(Redondear(parseFloat($("#lblSubTotalCarrito").text()) + parseFloat(response.d.precio)));
                            $("#lblCostoCarrito").text(parseFloat($("#lblSubTotalCarrito").text()));
                            $("#lblCantidadCarrito").text(parseInt($("#lblCantidadCarrito").text()) + 1);
                        }
                    },
                    error: function (response) {
                        MostrarError();
                    }
                });
            }

            function PopUpAgregarCarritoAtributo(idProducto) {
                $.ajax
                ({
                    type: "POST",
                    url: pageUrl + "/GetProducto",
                    data: "{'id_producto': '" + idProducto + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response.d.lstAtributoBE.length > 0) {

                            $("#spanNombreProducto").text(response.d.Nombre);
                            $("#hdIdProductoAtrib").val(idProducto);
                            $("#divAtributosProducto").html('');

                            $.each(response.d.lstAtributoBE, function (i, atributo) {
                                var select = $("<select class='validate[required]' id='ddlAtributo" + atributo.Id_Atributo + "' name='ddlAtributoP'/>")
                                select.append($("<option/>").attr("value", "-1").text(atributo.Nombre).attr("disabled", "disabled").attr("selected", "selected").attr("style", "display:none;"));
                                $.each(response.d.lstElemento_AtributoBE, function (j, elemento) {
                                    if (atributo.Id_Atributo == elemento.Id_Atributo)
                                        select.append($("<option/>").attr("value", elemento.Id_Elemento_Atributo).text(elemento.Nombre));
                                })

                                $("#divAtributosProducto").append(select);
                            })

                            $('#atributosProductoModal').modal('show');
                        }
                        else
                            AgregarCarrito(idProducto, '');
                    },
                    error: function (response) {
                        MostrarError();
                    }
                });
            }

            function AgregarProductoAtributos() {
                if ($('#atributosProductoModal').validationEngine('validate')) {
                    var atributos = '';

                    $("[name=ddlAtributoP]").each(function () {
                        atributos += $("#" + $(this).attr("id") + " option[value='-1']").text() + "," + $(this).children(':selected').text() + "," + $(this).val() + "|";
                    });

                    AgregarCarrito($("#hdIdProductoAtrib").val(), atributos);

                    $('#atributosProductoModal').modal('hide');
                }
            }

            function AgregarFavoritos(idProducto) {
                $.ajax
                ({
                    type: "POST",
                    url: pageUrl + "/AgregarFavoritos",
                    data: "{'id_producto': '" + idProducto + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if(response.d == 1)
                            jAlert("El producto fue agregado a favoritos.", "Favoritos");
                        else
                            jAlert("Por favor, primero ingrese con su usuario.", "Favoritos");
                    },
                    error: function (response) {
                        MostrarError();
                    }
                });
            }

            function BuscarProducto()
            {
                if ($("#navSearchInput").val() != "")
                    window.location.replace("/Buscar/" + $("#navSearchInput").val());
            }

            function BuscarPress()
            {
                if (event.keyCode == 13) {
                    BuscarProducto();
                }
            }

            function RecuperarPass()
            {
                if (!$('#inputEmailRegister').validationEngine('validate')) {
                    $.ajax
                    ({
                        type: "POST",
                        url: pageUrl + "/RecuperarPass",
                        data: "{'email': '" + $("#inputEmailRegister").val() + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (response) {
                            if(response.d == 1)
                                jAlert("Por favor, revise su correo para continuar con el proceso.", "Recuperar Contraseña");
                            else
                                jAlert("Correo inválido.", "Recuperar Contraseña");
                        },
                        error: function (response) {
                            MostrarError();
                        }
                    });
                }
            }

            function MostrarError() {
                jError('Ha ocurrido un error inesperado. Por favor, vuelva a intentar en un momento.', 'Error');
            }

            function Redondear(num)
            {
                return Math.round(num * 100) / 100
            }
        </script>

        <asp:ContentPlaceHolder ID="FooterContent" runat="server">
        </asp:ContentPlaceHolder>
    </form>
</body>
