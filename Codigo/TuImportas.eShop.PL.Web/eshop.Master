            <center><input id="btnRecuperarPass" type="button" class="btn btn-primary input-block-level bold higher" value="RESETEAR CONTRASEÑA" onclick="RecuperarPass()" /></center>
        </div>
    </div>

    <div id="atributosProductoModal" class="modal hide fade validationEngineContainer" tabindex="-1" role="dialog" aria-labelledby="atributosProductoModalLabel" aria-hidden="true"">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="atributosProductoModalLabel"><span class="light" id="spanNombreProducto"></span></h3>
        </div>
        <div class="modal-body" style="overflow-y:visible;text-align:center;">
            <div id="divAtributosProducto"></div>
            <input type="hidden" id="hdIdProductoAtrib"/>
            <center><input id="btnAgregarCarritoAtrib" type="button" class="btn btn-primary input-block-level bold higher" value="AGREGAR" onclick="AgregarProductoAtributos()" /></center>
        </div>
    </div>
</div> <!-- end of master-wrapper -->



